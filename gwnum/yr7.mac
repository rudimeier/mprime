	ystore	[srcreg+d1+32], ymm4		;; Save R16
	ystore	[srcreg+6*d2+d1], ymm3		;; Save R14

	vmovapd	ymm3, YMM_TMPS[0*32]		;; Load real-cols row #2
	vsubpd	ymm4, ymm3, ymm5		;; final R28
