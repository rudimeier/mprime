	vmulpd	ymm0, ymm3, ymm4		;; .975(i6-i10)
	vsubpd	ymm6, ymm6, ymm0		;; .782(i2-i14)+.434(i4-i12)-.975(i6-i10)
	vmulpd	ymm0, ymm1, ymm4		;; .434(i6-i10)
	vaddpd	ymm7, ymm7, ymm0		;; .975(i2-i14)-.782(i4-i12)+.434(i6-i10)

