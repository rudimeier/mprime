	movapd	r2, [rdi+off2+16]	;; pre_real/pre_imag
	mulpd	r2, r7			;; B2 = I2 * pre_real/pre_imag
	mulpd	r3, [rdi+off4]		;; A4 = A4 * pre_imag (final R4)
	mulpd	r4, [rdi+off4]		;; B4 = B4 * pre_imag (final I4)
	addpd	r1, r7			;; A2 = A2 + I2
	subpd	r2, r6			;; B2 = B2 - R2
	mulpd	r5, [rdi+off3]		;; A3 = A3 * pre_imag (final R3)
	mulpd	r8, [rdi+off3]		;; B3 = B3 * pre_imag (final I3)
	movapd	r6, [rdi+off1+16]	;; pre_real/pre_imag
	mulpd	r6, dest1		;; A1 = R1 * pre_real/pre_imag
	movapd	r7, [rdi+off1+16]	;; pre_real/pre_imag
	mulpd	r7, dest2		;; B1 = I1 * pre_real/pre_imag
	addpd	r6, dest2		;; A1 = A1 + I1
	subpd	r7, dest1		;; B1 = B1 - R1
	mulpd	r1, [rdi+off2]		;; A2 = A2 * pre_imag (final R2)
	mulpd	r2, [rdi+off2]		;; B2 = B2 * pre_imag (final I2)
	mulpd	r6, [rdi+off1]		;; A1 = A1 * pre_imag (final R1)
	mulpd	r7, [rdi+off1]		;; B1 = B1 * pre_imag (final I1)
	ENDM


x3cl_three_complex_first_fft MACRO srcreg,srcinc,d1
	movapd	xmm0, [srcreg][rbx]		;; R1
	movapd	xmm3, [srcreg+32][rbx]		;; R4
	movapd	xmm2, [srcreg+d1+16][rbx]	;; R3
	movapd	xmm5, [srcreg+d1+48][rbx]	;; R6
	movapd	xmm1, [srcreg+2*d1][rbx]	;; R2
	movapd	xmm4, [srcreg+2*d1+32][rbx]	;; R5
	x3c_premult xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7, 0
	x3c_fft xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7
	movapd	xmm6, [srcreg+d1][rbx]		;; R1
	movapd	xmm7, [srcreg+d1+32][rbx]	;; R4
	movapd	[srcreg], xmm2			;; Save R1
	movapd	[srcreg+32], xmm5		;; Save R2
	movapd	[srcreg+d1], xmm0		;; Save R3
	movapd	[srcreg+d1+16], xmm1		;; Save R4
	movapd	[srcreg+d1+32], xmm4		;; Save R5
	movapd	[srcreg+d1+48], xmm3		;; Save R6
	movapd	xmm1, [srcreg+16][rbx]		;; R2
	movapd	xmm4, [srcreg+48][rbx]		;; R5
	movapd	xmm2, [srcreg+2*d1+16][rbx]	;; R3
	movapd	xmm5, [srcreg+2*d1+48][rbx]	;; R6
	x3c_premult xmm6, xmm1, xmm2, xmm7, xmm4, xmm5, xmm0, xmm3, 96
	x3c_fft xmm6, xmm1, xmm2, xmm7, xmm4, xmm5, xmm0, xmm3
	movapd	[srcreg+16], xmm2		;; Save R1
	movapd	[srcreg+48], xmm5		;; Save R2
	movapd	[srcreg+2*d1], xmm6		;; Save R3
	movapd	[srcreg+2*d1+16], xmm1		;; Save R4
	movapd	[srcreg+2*d1+32], xmm4		;; Save R5
	movapd	[srcreg+2*d1+48], xmm7		;; Save R6
	lea	srcreg, [srcreg+srcinc]
	ENDM

; Same as above except we don't use the rbx offset as the FFT data has
; already been copied to the scratch area using the rbx offset.
x3cl_three_complex_fft MACRO srcreg,srcinc,d1
	movapd	xmm0, [srcreg]			;; R1
	movapd	xmm3, [srcreg+32]		;; R4
	movapd	xmm2, [srcreg+d1+16]		;; R3
	movapd	xmm5, [srcreg+d1+48]		;; R6
	movapd	xmm1, [srcreg+2*d1]		;; R2
	movapd	xmm4, [srcreg+2*d1+32]		;; R5
	x3c_premult xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7, 0
	x3c_fft xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7
	movapd	xmm6, [srcreg+d1]		;; R1
	movapd	xmm7, [srcreg+d1+32]		;; R4
	movapd	[srcreg], xmm2			;; Save R1
	movapd	[srcreg+32], xmm5		;; Save R2
	movapd	[srcreg+d1], xmm0		;; Save R3
	movapd	[srcreg+d1+16], xmm1		;; Save R4
	movapd	[srcreg+d1+32], xmm4		;; Save R5
	movapd	[srcreg+d1+48], xmm3		;; Save R6
	movapd	xmm1, [srcreg+16]		;; R2
	movapd	xmm4, [srcreg+48]		;; R5
	movapd	xmm2, [srcreg+2*d1+16]		;; R3
	movapd	xmm5, [srcreg+2*d1+48]		;; R6
	x3c_premult xmm6, xmm1, xmm2, xmm7, xmm4, xmm5, xmm0, xmm3, 96
	x3c_fft xmm6, xmm1, xmm2, xmm7, xmm4, xmm5, xmm0, xmm3
	movapd	[srcreg+16], xmm2		;; Save R1
	movapd	[srcreg+48], xmm5		;; Save R2
	movapd	[srcreg+2*d1], xmm6		;; Save R3
	movapd	[srcreg+2*d1+16], xmm1		;; Save R4
	movapd	[srcreg+2*d1+32], xmm4		;; Save R5
	movapd	[srcreg+2*d1+48], xmm7		;; Save R6
	lea	srcreg, [srcreg+srcinc]
	ENDM

;; This code is identical to the above except it is used in the single pass
;; all-complex FFTs and swizzles the inputs.
s3cl_three_complex_first_fft MACRO srcreg,srcinc,d1
	low_load xmm0, [srcreg][rbx], [srcreg+16][rbx] ;; R1
	low_load xmm3, [srcreg+32][rbx], [srcreg+48][rbx] ;; R4
	high_load xmm2, [srcreg+d1][rbx], [srcreg+d1+16][rbx] ;; R3
	high_load xmm5, [srcreg+d1+32][rbx], [srcreg+d1+48][rbx] ;; R6
	low_load xmm1, [srcreg+2*d1][rbx], [srcreg+2*d1+16][rbx] ;; R2
	low_load xmm4, [srcreg+2*d1+32][rbx], [srcreg+2*d1+48][rbx] ;; R5
	x3c_premult xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7, 0
	x3c_fft xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7
	high_load xmm6, [srcreg][rbx], [srcreg+16][rbx] ;; R2
	high_load xmm7, [srcreg+32][rbx], [srcreg+48][rbx] ;; R5
	movapd	[srcreg], xmm2			;; Save R1
	movapd	[srcreg+32], xmm5		;; Save R2
	low_load xmm2, [srcreg+d1][rbx], [srcreg+d1+16][rbx] ;; R1
	low_load xmm5, [srcreg+d1+32][rbx], [srcreg+d1+48][rbx]	;; R4
	movapd	[srcreg+d1], xmm0		;; Save R3
	movapd	[srcreg+d1+16], xmm1		;; Save R4
	movapd	[srcreg+d1+32], xmm4		;; Save R5
	movapd	[srcreg+d1+48], xmm3		;; Save R6
	high_load xmm0, [srcreg+2*d1][rbx], [srcreg+2*d1+16][rbx] ;; R3
	high_load xmm1, [srcreg+2*d1+32][rbx], [srcreg+2*d1+48][rbx] ;; R6
	x3c_premult xmm2, xmm6, xmm0, xmm5, xmm7, xmm1, xmm3, xmm4, 96
	x3c_fft xmm2, xmm6, xmm0, xmm5, xmm7, xmm1, xmm3, xmm4
	movapd	[srcreg+16], xmm0		;; Save R1
	movapd	[srcreg+48], xmm1		;; Save R2
	movapd	[srcreg+2*d1], xmm2		;; Save R3
	movapd	[srcreg+2*d1+16], xmm6		;; Save R4
	movapd	[srcreg+2*d1+32], xmm7		;; Save R5
	movapd	[srcreg+2*d1+48], xmm5		;; Save R6
	lea	srcreg, [srcreg+srcinc]
	ENDM

x3c_premult MACRO r1, r2, r3, r4, r5, r6, t1, t2, off
	movapd	t1, r1			;; Copy R1
	mulpd	r1, [rdi+off+16]	;; A1 = R1 * premul_real/premul_imag
	subpd	r1, r4			;; A1 = A1 - I1
	mulpd	r4, [rdi+off+16]	;; B1 = I1 * premul_real/premul_imag
	addpd	r4, t1			;; B1 = B1 + R1

	movapd	t1, r3			;; Copy R3
	mulpd	r3, [rdi+off+80]	;; A3 = R3 * premul_real/premul_imag
	subpd	r3, r6			;; A3 = A3 - I3
	mulpd	r6, [rdi+off+80]	;; B3 = I3 * premul_real/premul_imag
	addpd	r6, t1			;; B3 = B3 + R3
	mulpd	r1, [rdi+off]		;; A1 = A1 * premul_imag (new R1)
	mulpd	r4, [rdi+off]		;; B1 = B1 * premul_imag (new I1)

	movapd	t1, r2			;; Copy R2
	mulpd	r2, [rdi+off+48]	;; A2 = R2 * premul_real/premul_imag
	subpd	r2, r5			;; A2 = A2 - I2
	mulpd	r5, [rdi+off+48]	;; B2 = I2 * premul_real/premul_imag
	addpd	r5, t1			;; B2 = B2 + R2
	mulpd	r3, [rdi+off+64]	;; A3 = A3 * premul_imag (new R3)
	mulpd	r6, [rdi+off+64]	;; B3 = B3 * premul_imag (new I3)

	mulpd	r2, [rdi+off+32]	;; A2 = A2 * premul_imag (new R2)
	mulpd	r5, [rdi+off+32]	;; B2 = B2 * premul_imag (new I2)
	ENDM

;; Do a 3-complex FFT.  The input values are R1+R4i, R2+R5i, R3+R6i
;; A 3-complex FFT is:
;; Res1:  (R1+R2+R3) + (I1+I2+I3)i
;; Res2:  (R1-.5R2-.866I2-.5R3+.866I3) + (I1-.5I2+.866R2-.5I3-.866R3)i
;; Res3:  (R1-.5R2+.866I2-.5R3-.866I3) + (I1-.5I2-.866R2-.5I3+.866R3)i
x3c_fft MACRO r1, r2, r3, r4, r5, r6, t1, t2
	subpd	r2, r3			;; R2 - R3
	multwo	r3			;; R3 * 2
	addpd	r3, r2			;; R2 + R3
	mulpd	r2, XMM_P866		;; 0.866 * (R2 - R3)
	subpd	r5, r6			;; I2 - I3
	multwo	r6			;; I3 * 2
	addpd	r6, r5			;; I2 + I3
	mulpd	r5, XMM_P866		;; 0.866 * (I2 - I3)
	movapd	t1, XMM_HALF
	mulpd	t1, r3			;; 0.5 * (R2 + R3)
	addpd	r3, r1			;; R1 + R2 + R3 (final R1)
	subpd	r1, t1			;; (R1-.5R2-.5R3)
	movapd	t1, XMM_HALF
	mulpd	t1, r6			;; 0.5 * (I2 + I3)
	addpd	r6, r4			;; I1 + I2 + I3 (final I1)
	subpd	r4, t1			;; (I1-.5I2-.5I3)
	subpd	r1, r5			;; Final R2
	multwo	r5
	addpd	r5, r1			;; Final R3
	subpd	r4, r2			;; Final I3
	multwo	r2
	addpd	r2, r4			;; Final I2
	ENDM

x3cl_three_complex_last_unfft MACRO srcreg,srcinc,d1
	movapd	xmm0, [srcreg]		;; R1
	movapd	xmm1, [srcreg+32]	;; R2
	movapd	xmm2, [srcreg+d1]	;; R3
	movapd	xmm3, [srcreg+d1+32]	;; R4
	movapd	xmm4, [srcreg+2*d1]	;; R5
	movapd	xmm5, [srcreg+2*d1+32]	;; R6
	x3c_unfft xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7
	x3c_postmult xmm4, xmm5, xmm3, xmm1, xmm0, xmm2, xmm6, xmm7, 0
	movapd	[srcreg], xmm3		;; Save R1
	movapd	[srcreg+32], xmm1	;; Save R4
	movapd	[srcreg+2*d1], xmm0	;; Save R2
	movapd	[srcreg+2*d1+32], xmm2	;; Save R5
	movapd	xmm2, [srcreg+d1+16]	;; R3
	movapd	xmm3, [srcreg+d1+48]	;; R4
	movapd	[srcreg+d1+16], xmm6	;; Save R3
	movapd	[srcreg+d1+48], xmm7	;; Save R6
	movapd	xmm0, [srcreg+16]	;; R1
	movapd	xmm1, [srcreg+48]	;; R2
	movapd	xmm4, [srcreg+2*d1+16]	;; R5
	movapd	xmm5, [srcreg+2*d1+48]	;; R6
	x3c_unfft xmm0, xmm1, xmm2, xmm3, xmm4, xmm5, xmm6, xmm7
	x3c_postmult xmm4, xmm5, xmm3, xmm1, xmm0, xmm2, xmm6, xmm7, 96
	movapd	[srcreg+16], xmm0	;; Save R2
	movapd	[srcreg+48], xmm2	;; Save R5
	movapd	[srcreg+d1], xmm3	;; Save R1
	movapd	[srcreg+d1+32], xmm1	;; Save R4
	movapd	[srcreg+2*d1+16], xmm6	;; Save R3
	movapd	[srcreg+2*d1+48], xmm7	;; Save R6
	lea	srcreg, [srcreg+srcinc]
	ENDM

;; Do a 3-complex inverse FFT.  The input values are R1+R2i, R3+R4i, R5+R6i
;; A 3-complex inverse FFT is:
;; Res1:  (R1+R2+R3) + (I1+I2+I3)i
;; Res2:  (R1-.5R2+.866I2-.5R3-.866I3) + (I1-.5I2-.866R2-.5I3+.866R3)i
;; Res3:  (R1-.5R2-.866I2-.5R3+.866I3) + (I1-.5I2+.866R2-.5I3-.866R3)i
x3c_unfft MACRO r1, r2, r3, r4, r5, r6, t1, t2
	subpd	r3, r5			;; R2 - R3
	multwo	r5			;; R3 * 2
	addpd	r5, r3			;; R2 + R3
	mulpd	r3, XMM_P866		;; 0.866 * (R2 - R3)
	subpd	r4, r6			;; I2 - I3
	multwo	r6			;; I3 * 2
	addpd	r6, r4			;; I2 + I3
	mulpd	r4, XMM_P866		;; 0.866 * (I2 - I3)
	movapd	t1, XMM_HALF
	mulpd	t1, r5			;; 0.5 * (R2 + R3)
	addpd	r5, r1			;; R1 + R2 + R3 (final R1)
	subpd	r1, t1			;; (R1-.5R2-.5R3)
	movapd	t1, XMM_HALF
	mulpd	t1, r6			;; 0.5 * (I2 + I3)
	addpd	r6, r2			;; I1 + I2 + I3 (final I1)
	subpd	r2, t1			;; (I1-.5I2-.5I3)
	subpd	r1, r4			;; Final R3
	multwo	r4
	addpd	r4, r1			;; Final R2
	subpd	r2, r3			;; Final I2
	multwo	r3
	addpd	r3, r2			;; Final I3
	ENDM

x3c_postmult MACRO r1, r2, r3, r4, r5, r6, t1, t2, off
	movapd	t1, [rdi+off+80]	;; pre_real/pre_imag
	mulpd	t1, r5			;; A3 = R3 * pre_real/pre_imag
	movapd	t2, [rdi+off+80]	;; pre_real/pre_imag
	mulpd	t2, r6			;; B3 = I3 * pre_real/pre_imag
	addpd	t1, r6			;; A3 = A3 + I3
	subpd	t2, r5			;; B3 = B3 - R3

	movapd	r5, [rdi+off+48]	;; pre_real/pre_imag
	mulpd	r5, r3			;; A2 = R2 * pre_real/pre_imag
	movapd	r6, [rdi+off+48]	;; pre_real/pre_imag
	mulpd	r6, r4			;; B2 = I2 * pre_real/pre_imag
	addpd	r5, r4			;; A2 = A2 + I2
	subpd	r6, r3			;; B2 = B2 - R2
	mulpd	t1, [rdi+off+64]	;; A3 = A3 * pre_imag (final R3)
	mulpd	t2, [rdi+off+64]	;; B3 = B3 * pre_imag (final I3)

	movapd	r3, [rdi+off+16]	;; pre_real/pre_imag
	mulpd	r3, r1			;; A1 = R1 * pre_real/pre_imag
	movapd	r4, [rdi+off+16]	;; pre_real/pre_imag
	mulpd	r4, r2			;; B1 = I1 * pre_real/pre_imag
	addpd	r3, r2			;; A1 = A1 + I1
	subpd	r4, r1			;; B1 = B1 - R1
	mulpd	r5, [rdi+off+32]	;; A2 = A2 * pre_imag (final R2)
	mulpd	r6, [rdi+off+32]	;; B2 = B2 * pre_imag (final I2)
	mulpd	r3, [rdi+off]		;; A1 = A1 * pre_imag (final R1)
	mulpd	r4, [rdi+off]		;; B1 = B1 * pre_imag (final I1)
	ENDM

;; Do the brute-force multiplication of the 7 words near the half-way point.
;; These seven words were copied to an area 32-96 bytes before the FFT data.
;; This is done for zero-padded FFTs only.

xmult7	MACRO	src1, src2
	LOCAL	nozpad
	cmp	ZERO_PADDED_FFT, 0	;; Is this a zero-padded FFT?
	je	nozpad			;; No, skip 7 word multiply
	movsd	xmm0, Q [src1-40]	;; Result0 = word0 * word0
	mulsd	xmm0, Q [src2-40]
	movsd	xmm1, Q [src1-48]	;;	   + word1 * word-1
	mulsd	xmm1, Q [src2-72]
	addsd	xmm0, xmm1
	movsd	xmm2, Q [src1-56]	;;	   + word2 * word-2
	mulsd	xmm2, Q [src2-80]
	addsd	xmm0, xmm2
	movsd	xmm3, Q [src1-64]	;;	   + word3 * word-3
	mulsd	xmm3, Q [src2-88]
	addsd	xmm0, xmm3
	movsd	xmm1, Q [src1-72]	;;	   + word-1 * word1
	mulsd	xmm1, Q [src2-48]
	addsd	xmm0, xmm1
	movsd	xmm2, Q [src1-80]	;;	   + word-2 * word2
	mulsd	xmm2, Q [src2-56]
	addsd	xmm0, xmm2
	movsd	xmm3, Q [src1-88]	;;	   + word-3 * word3
	mulsd	xmm3, Q [src2-64]
	addsd	xmm0, xmm3
	movsd	ZPAD0, xmm0

	movsd	xmm0, Q [src1-40]	;; Result1 = word0 * word1
	mulsd	xmm0, Q [src2-48]
	movsd	xmm1, Q [src1-48]	;;	   + word1 * word0
	mulsd	xmm1, Q [src2-40]
	addsd	xmm0, xmm1
	movsd	xmm2, Q [src1-56]	;;	   + word2 * word-1
	mulsd	xmm2, Q [src2-72]
	addsd	xmm0, xmm2
	movsd	xmm3, Q [src1-64]	;;	   + word3 * word-2
	mulsd	xmm3, Q [src2-80]
	addsd	xmm0, xmm3
	movsd	xmm2, Q [src1-72]	;;	   + word-1 * word2
	mulsd	xmm2, Q [src2-56]
	addsd	xmm0, xmm2
	movsd	xmm3, Q [src1-80]	;;	   + word-2 * word3
	mulsd	xmm3, Q [src2-64]
	addsd	xmm0, xmm3
	movsd	ZPAD1, xmm0

	movsd	xmm0, Q [src1-40]	;; Result2 = word0 * word2
	mulsd	xmm0, Q [src2-56]
	movsd	xmm1, Q [src1-48]	;;	   + word1 * word1
	mulsd	xmm1, Q [src2-48]
	addsd	xmm0, xmm1
	movsd	xmm2, Q [src1-56]	;;	   + word2 * word0
	mulsd	xmm2, Q [src2-40]
	addsd	xmm0, xmm2
	movsd	xmm3, Q [src1-64]	;;	   + word3 * word-1
	mulsd	xmm3, Q [src2-72]
	addsd	xmm0, xmm3
	movsd	xmm3, Q [src1-72]	;;	   + word-1 * word3
	mulsd	xmm3, Q [src2-64]
	addsd	xmm0, xmm3
	movsd	ZPAD2, xmm0

	movsd	xmm0, Q [src1-40]	;; Result3 = word0 * word3
	mulsd	xmm0, Q [src2-64]
	movsd	xmm1, Q [src1-48]	;;	   + word1 * word2
	mulsd	xmm1, Q [src2-56]
	addsd	xmm0, xmm1
	movsd	xmm2, Q [src1-56]	;;	   + word2 * word1
	mulsd	xmm2, Q [src2-48]
	addsd	xmm0, xmm2
	movsd	xmm3, Q [src1-64]	;;	   + word3 * word0
	mulsd	xmm3, Q [src2-40]
	addsd	xmm0, xmm3
	movsd	ZPAD3, xmm0

	movsd	xmm0, Q [src1-48]	;; Result4 = word1 * word3
	mulsd	xmm0, Q [src2-64]
	movsd	xmm1, Q [src1-56]	;;	   + word2 * word2
	mulsd	xmm1, Q [src2-56]
	addsd	xmm0, xmm1
	movsd	xmm2, Q [src1-64]	;;	   + word3 * word1
	mulsd	xmm2, Q [src2-48]
	addsd	xmm0, xmm2
	movsd	ZPAD4, xmm0

	movsd	xmm0, Q [src1-56]	;; Result5 = word2 * word3
	mulsd	xmm0, Q [src2-64]
	movsd	xmm1, Q [src1-64]	;;	   + word3 * word2
	mulsd	xmm1, Q [src2-56]
	addsd	xmm0, xmm1
	movsd	ZPAD5, xmm0

	movsd	xmm0, Q [src1-64]	;; Result6 = word3 * word3
	mulsd	xmm0, Q [src2-64]
	movsd	ZPAD6, xmm0

nozpad:
	ENDM

