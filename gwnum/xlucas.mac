	multwo	xmm5
	addpd	xmm5, xmm4		;; R1 + R2 (final R1)
	movapd	[srcreg], xmm1		;; R1
	movapd	[srcreg+16], xmm0	;; R2
	movapd	[srcreg+32], xmm2	;; R3
	movapd	[srcreg+48], xmm3	;; R4
	movapd	[srcreg+d1], xmm5	;; R5
