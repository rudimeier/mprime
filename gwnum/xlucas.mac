	mulpd	r1, r3			;; A3 = R3 * premul_real/premul_imag
	movapd	r5, [rdi+16]		;; premul_real/premul_imag
	mulpd	r5, r7			;; B3 = I3 * premul_real/premul_imag
	subpd	r1, r7			;; A3 = A3 - I3
