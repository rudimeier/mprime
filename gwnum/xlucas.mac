g2cl_eight_reals_first_fft MACRO srcreg,srcinc,d1,dstreg,dstinc,e1
	movapd	xmm0, [srcreg][rbx]
	movapd	xmm1, [srcreg+d1][rbx]
	movapd	xmm2, [srcreg+16][rbx]
	movapd	xmm3, [srcreg+d1+16][rbx]
	movapd	xmm4, [srcreg+32][rbx]
	movapd	xmm5, [srcreg+d1+32][rbx]
