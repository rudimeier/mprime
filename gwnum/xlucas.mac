	mulpd	r4, [rdi]		;; A1 = A1 * premul_imag (new R1)
	mulpd	r8, [rdi]		;; B1 = B1 * premul_imag (new I1)

	movapd	r3, [rdi+80]		;; premul_real/premul_imag
	mulpd	r3, r2			;; A2 = R2 * premul_real/premul_imag
	movapd	r7, [rdi+80]		;; premul_real/premul_imag
	mulpd	r7, r6			;; B2 = I2 * premul_real/premul_imag
	subpd	r3, r6			;; A2 = A2 - I2
	addpd	r7, r2			;; B2 = B2 + R2
	mulpd	r1, [rdi]		;; A3 = A3 * premul_imag (new I3)
	mulpd	r5, [rdi]		;; B3 = B3 * premul_imag (new negR3)

	movapd	r2, [rdi+80]		;; premul_real/premul_imag
	mulpd	r2, mem4		;; A4 = R4 * premul_real/premul_imag
	movapd	r6, [rdi+80]		;; premul_real/premul_imag
	mulpd	r6, mem8		;; B4 = I4 * premul_real/premul_imag
	subpd	r2, mem8		;; A4 = A4 - I4
	addpd	r6, mem4		;; B4 = B4 + R4
	mulpd	r7, [rdi+64]		;; B2 = B2 * premul_imag (new I2)
	mulpd	r2, [rdi+64]		;; A4 = A4 * premul_imag (new I4)
	mulpd	r3, [rdi+64]		;; A2 = A2 * premul_imag (new R2)
	mulpd	r6, [rdi+64]		;; B4 = B4 * premul_imag (new negR4)

	subpd	r4, r5			;; R1 = R1 - negR3 (new R1)
	xprefetchw [pre1]
	multwo	r5			;; negR3 = negR3 * 2
	subpd	r8, r1			;; I1 = I1 - I3 (new I3)
	multwo	r1			;; I3 = I3 * 2
	subpd	r7, r2			;; I2 = I2 - I4 (new I4)
	multwo	r2			;; I4 = I4 * 2
	subpd	r3, r6			;; R2 = R2 - negR4 (new R2)
	multwo	r6			;; negR4 = negR4 * 2
	addpd	r5, r4			;; negR3 = R1 + negR3 (new R3)
	addpd	r1, r8			;; I3 = I1 + I3 (new I1)
	xprefetchw [pre1][pre2]
	addpd	r2, r7			;; I4 = I2 + I4 (new I2)
	addpd	r6, r3			;; negR4 = R2 + negR4 (new R4)

	subpd	r4, r3			;; R1 = R1 - R2 (final R2)
	movapd	destr2, r4
	multwo	r3
	addpd	r3, r4			;; R2 = R1 + R2 (final R1)
	movapd	r4, r1
	subpd	r1, r2			;; I1 = I1 - I2 (final I2)
	movapd	desti2, r1
	addpd	r2, r4			;; I2 = I1 + I2 (final I1)
	movapd	r4, r5
	subpd	r5, r7			;; R3 = R3 - I4 (final R3)
	addpd	r7, r4			;; I4 = R3 + I4 (final R4)
	movapd	r4, r8
	subpd	r8, r6			;; I3 = I3 - R4 (final I4)
	addpd	r6, r4			;; R4 = I3 + R4 (final I3)
	ENDM

