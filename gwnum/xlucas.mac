	movapd	[srcreg+48], xmm2	;; Save R1
	movapd	[srcreg+4*d1], xmm7	;; Save R2
	movapd	[srcreg+4*d1+16], xmm3	;; Save R3
	movapd	[srcreg+4*d1+32], xmm5	;; Save R4
